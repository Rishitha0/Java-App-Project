<section class="page-body">
  <% for (const creditCard of allCreditCards) { %>
    <div class="flex-fill">
      <div class="data-card">
        <div class="data-card-heading">
          <h3><%= creditCard.accountName %></h3>
        </div>
        <p>Current Balance: $<%= creditCard.currentBalance.toFixed(2) %></p>
      </div>
      <a href="/creditCard/<%= customer.customerId %>/<%= creditCard.accountNumber %>/details">View Card Details ></a>
    </div>
    <br>
  <% } %>
  <a href="/dashboard">Back to dashboard ></a>
</section>
